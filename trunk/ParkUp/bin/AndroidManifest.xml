<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thiagoadelino.parkup"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SplashScreen"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".EstacionamentoHistActivity"
            android:label="@string/title_activity_estacionamento_hist"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".EstacionamentoDetalheActivity"
            android:label="@string/title_activity_estacionamento_detalhe"
            android:parentActivityName=".EstacionamentoHistActivity" >
        </activity>
        <activity
            android:name=".ListaVeiculoActivity"
            android:label="@string/title_activity_lista_veiculo"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".DadosVeiculoActivity"
            android:label="@string/title_activity_dados_veiculo"
            android:parentActivityName=".ListaVeiculoActivity" >
        </activity>
        <activity
            android:name=".CheckinActivity"
            android:label="@string/title_activity_checkin"
            android:parentActivityName=".MainActivity" >
        </activity>
       
    </application>

</manifest>
